# Rotation speed in degrees per second (slower)
ROTATION_SPEED=90

# Rotation axis (x, y, or z)
ROTATION_AXIS=z

# Initial angle in degrees
INITIAL_ANGLE=0

# Target angle for rotation (in degrees)
TARGET_ANGLE=90

# Cube size
CUBE_SIZE=1.0

# Auto-stop at target angle (true/false)
STOP_AT_TARGET=true

# Rotation direction (1 for clockwise, -1 for counterclockwise)
ROTATION_DIRECTION=1
